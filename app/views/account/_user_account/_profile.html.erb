<h5 class="mb-4">PROFILE</h5>
<form>
  <div class="form-group">
    <label>Email</label>
    <input type="text" class="form-control text-primary" placeholder="Your Email" value="<%= current_user.email %>" readonly>
  </div>
  <div class="form-group">
    <label>First Name</label>
    <input type="text" class="form-control text-primary" placeholder="First Name" value="<%= current_user.first_name %>" readonly>
  </div>
  <div class="form-group">
    <label>Last Name</label>
    <input type="text" class="form-control text-primary" placeholder="Last Name" value="<%= current_user.last_name %>" readonly>
  </div>
  <div class="form-group">
    <label>Time Zone</label>
    <input type="text" class="form-control text-primary" placeholder="Your Time Zone" value="<%= current_user.time_zone %>" readonly>
  </div>
  <div class="form-group">
    <label>Role</label>
    <input type="text" class="form-control text-primary" placeholder="Your Role" value="<%= current_user.role %>" readonly>
  </div>
  <div class="form-group d-flex">
    <%= link_to 'Home', collections_path, class: 'btn btn-primary', data: { turbolinks: false } %>
    <%= link_to 'Edit details', edit_user_registration_path, class: 'btn btn-primary ml-auto' %>
  </div>
</form>
